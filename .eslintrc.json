{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "plugin:vue/strongly-recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "global-require": "off",
        "no-unused-expressions": [ "error", { "allowTaggedTemplates": true } ]
    },
    "settings": {
        "import/extensions": [
            ".vue",
            ".js"
          ]
    }
}